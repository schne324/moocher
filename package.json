{
  "name": "moocher",
  "version": "0.1.2",
  "description": "Web content scraper",
  "main": "index.js",
  "scripts": {
    "test": "nyc ava --serial",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/schne324/moocher.git"
  },
  "author": "Harris Schneiderman",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.25.0",
    "nyc": "^12.0.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "got": "^9.0.0",
    "promise-limit": "^2.7.0"
  },
  "nyc": {
    "checkCoverage": true,
    "reporter": [
      "text-summary",
      "html"
    ],
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "exclude": [
      "dist",
      "coverage",
      "test/**/*.js"
    ]
  }
}
